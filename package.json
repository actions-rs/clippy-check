        "@octokit/rest": "^16.34.1",
        "string-argv": "^0.3.1"
    },
    "devDependencies": {
        "@types/jest": "^24.0.21",
        "@types/node": "^12.12.5",
        "@zeit/ncc": "^0.20.5",
        "jest": "^24.9.0",
