        "npm-check-updates": "^7.0.1",
        "string-argv": "^0.3.1"
    },
    "devDependencies": {
        "@types/jest": "^26.0.0",
        "@types/node": "^14.0.13",
        "@zeit/ncc": "^0.22.3",
        "jest": "^26.0.1",
