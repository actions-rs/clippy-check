{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "allowJs": false,
        "outDir": "dist",
        "strict": true,
        "noImplicitAny": false,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "esModuleInterop": true
    },
    "include": [
        "src"
    ]
}
